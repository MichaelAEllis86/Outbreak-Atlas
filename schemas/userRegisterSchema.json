{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "user registration schema",
  "type": "object",
  "properties": {
    "username": { "type": "string" },
    "password": { "type": "string" },
    "first_name": { "type": "string" },
    "last_name": { "type": "string" },
    "age": { "type": "number" },
    "zipcode": { "type": "string" },
    "state": { "type": "string" }
  },
  "required": [
    "username",
    "password",
    "first_name",
    "last_name",
    "age",
    "zipcode",
    "state"
  ],
  "additionalProperties": false
}
